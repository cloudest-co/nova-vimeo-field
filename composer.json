{
    "name": "cloudest/nova-vimeo",
    "description": "A Laravel Nova 4.0 Vimeo field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cloudest\\Vimeo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cloudest\\Vimeo\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
